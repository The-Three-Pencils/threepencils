$("[data-price-per-month]").each((function(){var e=parseInt($(this).data("price-per-month"));var t=12*e;$(this).stop().prop("number",parseInt($(this).text())).css("width",$(this).width()).addClass("inlineBlock").animateNumber({number:t});var i=$(this).closest(".pad").find(".js-price");i.stop().prop("number",parseInt(i.text())).css("width",i.width()).addClass("inlineBlock").animateNumber({number:e});$(this).closest(".pad").find(".js-pricing-save").text("per year. Billed monthly")}));$("[data-text-per-month]").each((function(){$(this).text($(this).data("text-per-month"))}));$("[data-link-per-month]").each((function(){$(this).attr("href",$(this).data("link-per-month"))}))}else{
//ANNUALLY
$("[data-price-per-year]").each((function(){var e=parseInt($(this).data("price-per-year"));var t=12*parseInt($(this).closest(".pad").find("[data-price-per-month]").data("price-per-month"));$(this).stop().prop("number",parseInt($(this).text())).css("width",$(this).width()).addClass("inlineBlock").animateNumber({number:e});var i=$(this).closest(".pad").find(".js-price");i.stop().prop("number",parseInt(i.text())).css("width",i.width()).addClass("inlineBlock").animateNumber({number:Math.round(e/12)});$(this).closest(".pad").find(".js-pricing-save").text("billed annually. Save $"+(t-e))}));$("[data-text-per-year]").each((function(){$(this).text($(this).data("text-per-year"))}));$("[data-link-per-year]").each((function(){$(this).attr("href",$(this).data("link-per-year"))}))}}));
